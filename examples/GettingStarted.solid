func fib(x)
    when x < 3 then 1
    otherwise fib(x-1) + fib(x-2);

fib(10);

native printc(x);
func printstars(n)
    while i < n for i = 1 step 1 do
        printc(42); # 42 = '*'

printstars(100);

operator binary> 10 (L R)
    R < L;

20 > 10;

operator unary$ (x)
    x + 1;

$$$2;

operator unary!(x)
    when x then 0
    otherwise 1;

operator unary-(x)
    0-x;

operator binary| 5 (L R)
    when L then 1
    otherwise when R then 1
    otherwise 0;
